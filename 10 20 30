#include <iostream>
using namespace std;

int main() {
    int n;
    cout << "Ededlerin sayÄ±nÄ± daxil edin: ";
    cin >> n; 
    int array[n];
    cout << "Ededleri daxil edin: ";
    for (int i = 0; i < n; i++) {
        cin >> array[i];  
    }
    for (int i = 0; i < n; i++) {
        int cutluk = 0; 
        for (int j = i + 1; j < n; j++) {
            if (array[i] == array[j]) {
                cutluk++;  
            }
        }
        if (cutluk > 0) {
            cout << array[i] << " - " << array[i] << " " << cutluk + 1 << " dene" << endl;
        }
    }

    return 0;
}
